{
  "name": "igf-agent",
  "version": "1.0.0",
  "description": "Frida agent",
  "private": true,
  "main": "agent/index.ts",
  "scripts": {
    "build:springboard": "frida-compile fruity/springboard.ts -o dist/springboard.js",
    "build:android": "frida-compile droid/index.ts -o dist/droid.js",
    "build:ios": "frida-compile fruity/index.ts -o dist/fruity.js",
    "build:eval": "frida-compile eval.ts -o dist/eval.js",
    "node:flags": "node --experimental-strip-types",
    "prepare": "npm run node:flags scripts/prepare.ts",
    "dev": "npm run node:flags scripts/watch.ts",
    "test": "npm run node:flags -- --test tests/*.ts",
    "type:flags": "tsgo --declaration --experimentalDecorators --emitDeclarationOnly --outDir types --target es2019 --skipLibCheck",
    "types:ios": "npm run type:flags -- fruity/index.ts",
    "types:android": "npm run type:flags -- droid/index.ts",
    "types:springboard": "npm run type:flags -- fruity/springboard.ts",
    "types:eval": "npm run type:flags -- eval.ts"
  },
  "devDependencies": {
    "@types/frida-gum": "^19.0.0",
    "@types/node": "^18.14.0",
    "@typescript/native-preview": "^7.0.0-dev.20251106.1",
    "frida": "^17.0.7",
    "frida-compile": "^18.0.0"
  },
  "dependencies": {
    "frida-objc-bridge": "^8.0.4"
  },
  "type": "module"
}
